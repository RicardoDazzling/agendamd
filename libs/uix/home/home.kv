

<Home>:
    MDBoxLayout:
        size: root.size
        pos: root.pos
        spacing: "20dp"
        MDNavigationRail:
            id: nav_rail
            type: "unselected"
            MDNavigationRailItem:
                id: dashboard_item
                on_active: root.goto_dashboard(*args)
                MDNavigationRailItemIcon:
                    icon: "view-dashboard-outline"
            MDNavigationRailItem:
                id: task_item
                on_active: root.goto_task(*args)
                MDNavigationRailItemIcon:
                    icon: "list-box-outline"
            MDNavigationRailItem:
                id: finished_item
                on_active: root.goto_finished(*args)
                MDNavigationRailItemIcon:
                    icon: "archive-outline"
            MDNavigationRailItem:
                id: group_item
                on_active: root.goto_group(*args)
                MDNavigationRailItemIcon:
                    icon: "account-group-outline"
            MDNavigationRailItem:
                id: inbox_item
                on_active: root.goto_inbox(*args)
                MDNavigationRailItemIcon:
                    icon: "inbox-outline"
        MainScreenManager:
            Dashboard:
                name: "dashboard"
